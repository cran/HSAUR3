        mvtnorm (>= 0.9-9996), partykit (>= 0.8-0), quantreg (>= 5.05),
        randomForest (>= 4.6-7), rmeta (>= 2.16), sandwich (>= 2.3-0),
        scatterplot3d (>= 0.3-34), sp (>= 1.0-14), TH.data (>= 1.0-2),
        vcd (>= 1.3-1), wordcloud (>= 2.4), HSAUR2
